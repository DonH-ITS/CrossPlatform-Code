<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="NavShellBasic.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:NavShellBasic"
    Shell.FlyoutBehavior="Disabled"
    Title="NavShellBasic">

    <ShellContent Title="Home" ContentTemplate="{DataTemplate local:HomePage}" Route="home" />
    <ShellContent Title="Details" ContentTemplate="{DataTemplate local:DetailsPage}" Route="details" />
    <ShellContent Title="MainPage" ContentTemplate="{DataTemplate local:HomePage}" Route="main" />

</Shell>
