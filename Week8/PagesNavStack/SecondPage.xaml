<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PagesNavStack.SecondPage"
             Title="SecondPage">

    <Shell.BackButtonBehavior>
        <BackButtonBehavior IsVisible="False" />
    </Shell.BackButtonBehavior>


    <StackLayout>
        <Label x:Name="namelbl" HorizontalOptions="CenterAndExpand"/>
        <Label Text="Welcome to Page 2!" HorizontalOptions="CenterAndExpand" />
        <Button Text="Go Back" Clicked="OnBackButtonClicked" />

        <Label x:Name="afterpop"/>
    </StackLayout>
</ContentPage>