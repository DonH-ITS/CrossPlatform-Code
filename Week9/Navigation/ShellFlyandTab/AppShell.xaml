<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ShellFlyTab.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ShellFlyTab"
    Shell.FlyoutBehavior="Flyout"
    Title="ShellFlyTab">

   <FlyoutItem Title="Flyout One" FlyoutIcon="{StaticResource IconOne}"> 
        <Tab Title="Tab 1" Icon="{StaticResource IconOne}">
            <ShellContent
            Title="One"
            ContentTemplate="{DataTemplate local:MainPage}"
            Route="OnePage" />
        </Tab>
        <Tab Title="Tab 2" Icon="{StaticResource IconTwo}">
            <ShellContent
            Title="Two"
            ContentTemplate="{DataTemplate local:NewPage1}"
            Route="TwoPage" />
        </Tab>
    </FlyoutItem>
    <FlyoutItem Title="Flyout Two" FlyoutIcon="{StaticResource IconOne}">
        <Tab Title="Tab 1" Icon="{StaticResource IconOne}">
            <ShellContent
            Title="One"
            ContentTemplate="{DataTemplate local:MainPage}"
            Route="OnePage" />
        </Tab>
        <Tab Title="Tab 2" Icon="{StaticResource IconTwo}">
            <ShellContent
            Title="Two"
            ContentTemplate="{DataTemplate local:NewPage1}"
            Route="TwoPage" />
        </Tab>
    </FlyoutItem>

</Shell>
